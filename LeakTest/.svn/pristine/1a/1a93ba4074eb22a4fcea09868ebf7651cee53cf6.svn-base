<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/dp_10">

    <EditText
        android:id="@+id/edtSerialNum"
        android:layout_width="@dimen/dp_182"
        android:layout_height="@dimen/dp_40"
        android:layout_margin="@dimen/dp_10"
        android:background="@drawable/chart_edittext_bg"
        android:hint="@string/device_sn"
        android:inputType="number"
        android:paddingStart="@dimen/dp_6"
        android:paddingEnd="@dimen/dp_6"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        tools:ignore="Autofill,LabelFor" />

    <Button
        android:id="@+id/btnSn"
        style="@style/commonButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_10"
        android:text="写入序列号"
        app:layout_constraintBottom_toBottomOf="@+id/edtSerialNum"
        app:layout_constraintStart_toEndOf="@+id/edtSerialNum"
        app:layout_constraintTop_toTopOf="@+id/edtSerialNum" />

    <Button
        android:id="@+id/btnAgingTest"
        style="@style/commonButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_2"
        android:text="启动老化测试程序"
        app:layout_constraintBottom_toBottomOf="@+id/btnSn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnSn"
        app:layout_constraintTop_toTopOf="@+id/btnSn" />

    <Button
        android:id="@+id/btnStartLauncher"
        style="@style/commonButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_2"
        android:text="显示桌面Launcher"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>