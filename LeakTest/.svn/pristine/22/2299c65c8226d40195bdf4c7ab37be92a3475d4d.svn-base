package com.cnlaunch.physics.usb;

class FTDIUsbId extends UsbId  {
    int mBcdDevice;
    int mNumOfChannels;
    FTDICHIPTYPE mType;
    FTDIUsbId(int vid, int pid, int bcdDevice, int numOfChannels, FTDICHIPTYPE type) {
        super(vid,pid);
        mBcdDevice = bcdDevice;
        mNumOfChannels = numOfChannels;
        mType = type;
    }
}
