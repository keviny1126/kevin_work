<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_3" />
            <gradient
                android:angle="90"
                android:centerColor="@color/color_cad9d1"
                android:centerY="0.5"
                android:endColor="@color/color_EEEEEE"
                android:startColor="@color/color_EEEEEE" />
        </shape>
    </item>
    <item android:bottom="@dimen/dp_2"
        android:top="@dimen/dp_2">
        <shape android:shape="rectangle">
            <gradient
                android:angle="0"
                android:centerColor="@color/color_cad9d1"
                android:centerY="0.5"
                android:endColor="@color/color_EEEEEE"
                android:startColor="@color/color_EEEEEE" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/dp_2"
        android:left="@dimen/dp_2"
        android:right="@dimen/dp_2"
        android:top="@dimen/dp_2">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/dp_3" />
        </shape>
    </item>
</layer-list>