<resources>

    <style name="loadingDialogStyle" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 对话框是否有遮盖 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="dialogActivityTheme" parent="Theme.AppCompat.Light.Dialog">
        <!--设置dialog的背景-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--设置Dialog的windowFrame框为无-->
        <item name="android:windowFrame">@null</item>
        <!--设置无标题-->
        <item name="windowNoTitle">true</item>
        <!--是否浮现在activity之上，设置成true在部分机型上高度可能显示不全-->
        <item name="android:windowIsFloating">false</item>
        <!--是否半透明-->
        <item name="android:windowIsTranslucent">true</item>
        <!--设置窗口内容不覆盖-->
        <item name="android:windowContentOverlay">@null</item>
        <!--设置动画-->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <!--背景是否模糊显示-->
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="commonButton">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/sp_10</item>
        <item name="android:paddingStart">@dimen/dp_10</item>
        <item name="android:paddingEnd">@dimen/dp_10</item>
        <item name="android:background">@drawable/selector_common_button</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="coeffDialogText">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/sp_9</item>
        <item name="android:paddingStart">@dimen/dp_4</item>
        <item name="android:paddingEnd">@dimen/dp_4</item>
        <item name="android:background">@drawable/shape_edittext</item>
    </style>

    <style name="testButtonStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/sp_10</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingStart">@dimen/dp_4</item>
        <item name="android:paddingEnd">@dimen/dp_4</item>
        <item name="android:background">@drawable/btn_star_stop_nor</item>
        <item name="android:minWidth">@dimen/dp_78</item>
        <item name="android:minHeight">@dimen/dp_32</item>
    </style>

    <style name="ProgressBar" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:maxHeight">@dimen/dp_15</item>
        <item name="android:minHeight">@dimen/dp_10</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@drawable/progressbar_bg</item>
    </style>

    <style name="mainTextStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginBottom">@dimen/dp_9</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="settingLeftTextStyle">
        <item name="android:textColor">@color/color_selector_table</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_setting_table_bg</item>
    </style>

    <style name="commonTextStyle">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="configTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="configEdittextStyle">
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:minHeight">@dimen/dp_28</item>
        <item name="android:paddingStart">@dimen/dp_9</item>
        <item name="android:paddingEnd">@dimen/dp_9</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/common_green_bg</item>
    </style>

    <style name="saveDataTextStyle">
        <item name="android:textColor">@color/color_777777</item>
        <item name="android:textSize">@dimen/sp_10</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="Theme.picker" parent="Theme.AppCompat.Light">
        <!--分割线颜色-->
        <item name="colorControlNormal">@color/color_00A65C</item>
        <item name="android:editTextStyle">@style/Widget.EditText.White</item>
    </style>

    <style name="Widget.EditText.White" parent="@android:style/Widget.EditText">
        <!--文字颜色-->
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/sp_12</item>
    </style>

    <style name="dataBottomBtnStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:background">@drawable/selector_common_button</item>
        <item name="android:paddingBottom">@dimen/dp_2</item>
        <item name="android:paddingStart">@dimen/dp_4</item>
        <item name="android:paddingEnd">@dimen/dp_4</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="zyMainTextStyle">
        <item name="android:textColor">@color/color_313131</item>
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="zyCommonTextStyle">
        <item name="android:textColor">@color/color_313131</item>
        <item name="android:textSize">@dimen/sp_10</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="zyTestTitleTextStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/sp_7</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="zyConfigTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_727272</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="zyConfigEdittextStyle" parent="configEdittextStyle">
        <item name="android:background">@drawable/zy_common_grey_bg</item>
        <item name="android:textColor">@color/color_313131</item>
    </style>
    <style name="zyDataBottomBtnStyle">
        <item name="android:textColor">@color/color_313131</item>
        <item name="android:textSize">@dimen/sp_11</item>
        <item name="android:background">@drawable/selector_common_button_grey</item>
        <item name="android:paddingStart">@dimen/dp_2</item>
        <item name="android:paddingEnd">@dimen/dp_2</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="zySettingLeftTextStyle">
        <item name="android:textColor">@color/color_313131</item>
        <item name="android:textSize">@dimen/sp_13</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/zy_selector_setting_table_bg</item>
    </style>
</resources>